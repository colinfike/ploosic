<%= content_for :content do %>
  
  <% @playlist.tracks.each do |track| %>
    <%= render_track(track) %>
  <% end %>
  <iframe id='soundcloud-player' width="250" height="150" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=<%= @url %>&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false&amp;visual=true"></iframe>
<% end %>
